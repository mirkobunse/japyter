{
    "$schema": "http://json-schema.org/draft-03/schema",
    "type": "object",
    "extends" : {
        "$ref" : "request.json"
    },
    "properties": {
        "code": {
            "type": "string",
            "required": true
        },
        "silent": {
            "type": "boolean",
            "default": false
        },
        "store_history": {
            "type": "boolean"
        },
        "user_expressions": {
            "$ref": "user_expressions.json"
        },
        "allow_stdin": {
            "type": "boolean"
        },
        "stop_on_error": {
            "type": "boolean"
        }
    }
}
